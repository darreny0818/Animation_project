}
void draw () {
  background(#499FD3);
  fill(#43AA24);
  //ellipse(x1, 500, 400, 400);
  //ellipse(x2, 500, 400, 400);
  //ellipse(x3, 500, 400, 400);
  
  noStroke();
  fill (255);
  ellipse(x4, 150, 100, 100);
  ellipse(x4, 200, 100, 100);
  ellipse(x4-50, 200, 100, 100);
  ellipse(x4+50, 200, 100, 100);
  
  stroke(0);
  fill(#4955D3);
  rect(0, 575, 800, 25);
  fill(#BDBEBF);
  rect(0, 475, 800, 100);
  
  pushMatrix();
  translate(quidditch, 100);
  quidditch();
  popMatrix();
  
  pushMatrix();
  translate(hogwarts, 100);
  hogwarts ();
  popMatrix();
  
  pushMatrix();
  translate(harry, harry1);
  harry ();
  popMatrix();
  
  pushMatrix ();
  translate (harry3, harry2);
  harry2 ();
  popMatrix();
 
  
  x1 = x1 + 5;
  x2 = x2 + 5;
  x3 = x3 + 5;
  x4 = x4 + 5;
  hogwarts = hogwarts + 3;
  quidditch = quidditch + 3;
  harry1 = harry1 + 1; 
  harry = harry - 1;
  harry2 = harry2 + 1;
  harry3 = harry3 - 1;
  
  if (x1 >= 1000) {
    x1 = -200;
  }
  if (x2 >= 1000) {
    x2 = -200;
  }
  if (x3 >= 1000) {
    x3 = -200;
  }
  if (x4 >= 900) {
    x4 = -100;
  }
  if (hogwarts >= 800) {
    hogwarts = -1430;
  }
  if (quidditch >= 800) {
    quidditch = -1430;
  }
  if (harry1 >=6) {
    harry1 = 0;
  }
  if (harry <= -450) {
    harry = 450;
  }
  if (harry2 >=56) {
    harry2  = 50;
  }
  if (harry3 <= -450) {
    harry3 = 450;
  }
}
